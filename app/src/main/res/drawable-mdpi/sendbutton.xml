<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="30dp" />
            <stroke android:width="1dip" android:color="#26A69A" />
            <solid android:color="#26A69A"/>
            <solid android:color="#26A69A"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="30dp" />
            <stroke android:width="1dip" android:color="#80CBC4" />
            <solid android:color="#80CBC4"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="30dp" />
            <stroke android:width="1dip" android:color="#80CBC4" />
            <gradient android:angle="0" android:startColor="#80CBC4" android:endColor="#80CBC4" />
        </shape>
    </item>
</selector>